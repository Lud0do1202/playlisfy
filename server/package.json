{
    "name": "playlisfy-server",
    "version": "1.0.0",
    "description": "API of Playlisfy application",
    "main": "server.js",
    "scripts": {
        "lint": "eslint . --max-warnings=0",
        "lint-fix": "eslint . --fix",
        "prettier": "prettier --write '**/*.{js,ts,json,md,mjs}'",
        "format": "npm run prettier && npm run lint",
        "dev": "tsnd --respawn src/server.ts",
