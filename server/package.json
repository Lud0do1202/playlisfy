        "@types/node": "^22.10.5",
        "eslint": "^9.18.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.14.0",
        "prettier": "^3.4.2",
        "sequelize-cli": "^6.6.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.19.1"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
